---
    import Header from './';
    import Logo from '@assets/logo.svg?raw';
    
    const currentPath = Astro.url.pathname;

    const pathNav = [
        {
            name: 'Works',
            url: '/works'
        },
        {
            name: 'Services',
            url: '/services'
        },
        {
            name: 'About',
            url: '#'
        }
    ]
    const socialData = [
        {
            name: 'Facebook',
            url: '#'
        },
        {
            name: 'Instagram',
            url: '#'
        },
        {
            name: 'LinkedIn',
            url: '#'
        }
    ]
    const mailLink = {
            name: 'wethebrand@studio.com',
            url: 'wethebrand@studio.com'
        }
---


<Header
    currPath={currentPath}
    navigation={pathNav}
    socialData={socialData}
    contact={mailLink}
    client:load
>
    <Fragment set:html={Logo} slot='logo'></Fragment>
</Header>